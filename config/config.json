{
    "autoclear": false,
    "integrations": {
        "explorer": true,
        "editcmd": "notepad {}"
    },
    "showfulldir": true,
    "listviewlimit": 250,
    "matchignorecase": true,
    "dollarsuffixisendswith": true,
    "qmsuffixiscontains": true,
    "ignorewhitespace": true,
    "hideafteruiinteraction": true,
    "basedir": "C:/.dev",
    "matchpath": false,
    "maxitemsforautoupdate": 1000,
    "native": {
        "maxitemsforautoupdate": 50000,
        "autofreebuffer": false,
        "autofreetimeout": 300
    },
    "vim": {
        "enabled": true,
        "list_up": "j",
        "list_down": "k",
        "half_viewport_up": "^j",
        "half_viewport_down": "^k",
        "bot": "+g",
        "top": "g",
        "open_explorer": "e",
        "edit_file": "f",
        "yank_path": "y",
        "yank_name": "+y"
    }
